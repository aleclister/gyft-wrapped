            .d-sm-flex
              a(href='#' style='max-width: 18rem;').card.card-curved-body.card-hover.me-4.mb-4
                .card-floating-icon: i.ai-instagram
                .card-img-top.card-img-gradient
                  img(src='../img/team/01.jpg' alt='Sarah Cole')
                .card-body.text-center
                  h3.h6.card-title.mb-2 Sarah Cole
                  p.fs-xs.text-body.mb-0 Social d-flex strategist
              .card.card-curved-body.card-hover.border-0.shadow.mb-4(style='max-width: 18rem;')
                a(href='#').card-floating-icon: i.ai-mail
                .card-img-top.card-img-gradient
                  img(src='../img/team/02.jpg' alt='Charlie Welch')
                .card-body.text-center
                  h3.h6.card-title.mb-2 Charlie Welch
                  p.fs-xs.text-body.mb-0 Financial director
          #html1.tab-pane.fade(role='tabpanel')
            pre.line-numbers
              code(class='lang-html').
                &lt;!-- Team Style 1: Border + whole card is a link --&gt;
                &lt;a class=&quot;card card-curved-body card-hover&quot; href=&quot;#&quot;&gt;
                  &lt;div class=&quot;card-floating-icon&quot;&gt;
                    &lt;i class=&quot;ai-instagram&quot;&gt;&lt;/i&gt;
