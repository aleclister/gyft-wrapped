  // Navbar (Solid background + shadow)
  // Remove "navbar-sticky" class to make navigation bar scrollable with the page.
  header.header.navbar.navbar-expand-lg.navbar-light.bg-light.navbar-shadow.navbar-sticky(data-scroll-header)
    .navbar-search.bg-light
      .container.d-flex.flex-nowrap.align-items-center
        i.ai-search.fs-xl
        input(type='text' placeholder='Search site').form-control.form-control-xl.navbar-search-field
        .d-flex.align-items-center
          span.text-muted.fs-xs.d-none.d-sm-inline Close
          button(type='button' data-bs-toggle='search').btn-close.p-2
    .container.px-0.px-xl-3
      button(type='button' data-bs-toggle='offcanvas' data-bs-target='#primaryMenu').navbar-toggler.ms-n2.me-4
        span.navbar-toggler-icon
      a(href='index.html').navbar-brand.flex-shrink-0.order-lg-1.mx-auto.ms-lg-0.pe-lg-2.me-lg-4
        img(src='img/logo/logo-dark.png' alt='Around' width='153').d-none.d-lg-block
        img(src='img/logo/logo-icon.png' alt='Around' width='58').d-lg-none
      .d-flex.align-items-center.order-lg-3.ms-lg-auto
        .navbar-tool
          a(href='#' data-bs-toggle='search').navbar-tool-icon-box.me-lg-2
            i.ai-search
        div(style='height: 30px;').d-lg-block.d-none.border-start.me-4
        .d-lg-block.d-none.text-nowrap
          a(href='#').btn-social.bs-outline.bs-facebook.me-2
            i.ai-facebook
          a(href='#').btn-social.bs-outline.bs-twitter.me-2
            i.ai-twitter
          a(href='#').btn-social.bs-outline.bs-instagram.me-2
            i.ai-instagram
          a(href='#').btn-social.bs-outline.bs-pinterest
            i.ai-pinterest
      #primaryMenu.offcanvas.offcanvas-collapse.order-lg-2
        .offcanvas-header.navbar-shadow
          h5.mt-1.mb-0 Menu
          button(type='button' data-bs-dismiss='offcanvas' aria-label='Close').btn-close.lead
        .offcanvas-body
          //- partial/menu-primary
          include partials/menu-primary
        .offcanvas-footer.justify-content-center.border-top
          a(href='#').btn-social.bs-lg.bs-outline.bs-facebook.me-3
            i.ai-facebook
          a(href='#').btn-social.bs-lg.bs-outline.bs-twitter.me-3
            i.ai-twitter
          a(href='#').btn-social.bs-lg.bs-outline.bs-instagram.me-3
            i.ai-instagram
          a(href='#').btn-social.bs-lg.bs-outline.bs-pinterest
            i.ai-pinterest


