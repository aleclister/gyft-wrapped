      .row
        .col-xl-7.col-lg-8.col-md-10
          .bg-light.rounded-3.shadow-lg.py-5.px-4
            form.py-sm-2.px-sm-3
              h1.text-center.mb-4 Reserve your spot
              .row
                .col-sm-6.mb-3.pb-1
                  label(for='rf-space').form-label Space
                  select#rf-space.form-select
                    option(value='Coworking main room') Coworking main room
                    option(value='Coworking terrace') Coworking terrace
                    option(value='Skype room 1' selected) Skype room 1
                    option(value='Skype room 2') Skype room 2
                    option(value='Meeting room 1') Meeting room 1
                    option(value='Meeting room 2') Meeting room 2
                    option(value='Gaming room') Gaming room
                .col-sm-6.mb-3.pb-1
                  label(for='rf-date').form-label Date and time
                  .input-group
                    input(type='text' placeholder='Choose date / time' data-datepicker-options='{"enableTime": true, "altInput": true, "altFormat": "F j, Y H:i", "dateFormat": "Y-m-d H:i", "minDate": "today"}')#rf-date.form-control.appended-form-control.date-picker.rounded
                    i.ai-calendar.position-absolute.top-50.end-0.translate-middle-y.me-3
                .col-sm-6.mb-3.pb-1
                  label(for='rf-company-size').form-label Company size
                  input(type='number' min='1' value='1' placeholder='How many people')#rf-company-size.form-control
                .col-sm-6.mb-3.pb-1
                  label(for='rf-email').form-label Email address
                  input(type='email' placeholder='Your email')#rf-email.form-control
              .row.align-items-center.pt-2
                .col-sm-6.mb-4.mb-sm-0
                  a(href='tel:+15262200459')
                    i.ai-phone.fs-xl.me-2
