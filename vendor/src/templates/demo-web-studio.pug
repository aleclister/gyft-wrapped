        .text-nowrap.me-3
          span.frame-browser-button.bg-danger
          span.frame-browser-button.bg-warning
          span.frame-browser-button.bg-success
        span.tns-carousel-label.fs-sm.text-light.opacity-75 https://mobileapp.com
      .frame-browser-body
        .tns-carousel-inner(data-carousel-options='{"mode": "gallery", "controls": false, "nav": false, "autoplay": true, "autoplayTimeout": 6000}')
          div(data-carousel-label='https://mobileapp.com')
            img(src='img/demo/web-studio/slider/01.jpg' alt='Screenshot').rounded
          div(data-carousel-label='https://bookingapp.com')
            img(src='img/demo/web-studio/slider/02.jpg' alt='Screenshot').rounded
