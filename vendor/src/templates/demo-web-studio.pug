      svg(xmlns='http://www.w3.org/2000/svg' viewBox='0 0 3000 185.4')
        path(fill='currentColor' d='M3000,0v185.4H0V0c496.4,115.6,996.4,173.4,1500,173.4S2503.6,115.6,3000,0z')
    
    // Text
    .container.position-relative.zindex-5.text-center.pt-6.pt-lg-7
      h2.fs-xl.fw-normal.text-light Award winning web design agency
      h1.text-light Get your website done

    .d-block.d-sm-none(style='height: 100px;')
    .d-none.d-sm-block.d-lg-none(style='height: 300px;')
    .d-none.d-lg-block(style='height: 400px;')


