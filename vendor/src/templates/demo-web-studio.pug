extends partials/layout

//- Override default config variables
append config
  - var vendorStyles = ['tiny-slider/dist/tiny-slider.css']
  - var vendorScripts = ['tiny-slider/dist/min/tiny-slider.js', 'jarallax/dist/jarallax.min.js', 'imagesloaded/imagesloaded.pkgd.min.js', 'shufflejs/dist/shuffle.min.js']
  - var parentMenu = 'Demos'
  - var pageTitle = 'Web Studio'

//- Navbar
block navbar
  include partials/navbar-floating-light

//- Page Content
